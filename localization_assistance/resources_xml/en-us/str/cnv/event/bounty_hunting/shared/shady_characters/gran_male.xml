<stb_file>
	<file_info>
		<strs>8</strs>
		<file_name>/resources/en-us/str/cnv/event/bounty_hunting/shared/shady_characters/gran_male.stb</file_name>
	</file_info>
	<file_content>
		<text bitflag="3393" id1="1113526" id2="17" version="1065353216">Stop! I will tell you what I know.</text>
		<text bitflag="3393" id1="1113527" id2="17" version="1065353216">I will not be treated this way!</text>
		<text bitflag="3393" id1="1113529" id2="17" version="1065353216">If it keeps the attention off me, I'll talk.</text>
		<text bitflag="3393" id1="1113530" id2="17" version="1065353216">You just got on my last nerve!</text>
		<text bitflag="3393" id1="1113531" id2="17" version="1065353216">I have nothing to say.</text>
		<text bitflag="3393" id1="1113533" id2="17" version="1065353216">Much appreciated. I will tell you what I can.</text>
		<text bitflag="3393" id1="1113534" id2="17" version="1065353216">I am not so easily turned!</text>
		<text bitflag="3393" id1="1113535" id2="17" version="1065353216">Generous, but pointless.</text>
	</file_content>
</stb_file>
