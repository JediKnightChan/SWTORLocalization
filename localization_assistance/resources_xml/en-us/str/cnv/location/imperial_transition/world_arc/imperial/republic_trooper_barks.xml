	<file_content>
		<text bitflag="1345" id1="661842" id2="17" version="1065353216">For the Republic!</text>
		<text bitflag="1345" id1="661843" id2="17" version="1065353216">All units, maximum firepower!</text>
		<text bitflag="1345" id1="661844" id2="17" version="1065353216">Drop your weapons and surrender!</text>
		<text bitflag="1345" id1="661845" id2="17" version="1065353216">Blast them!</text>
		<text bitflag="1345" id1="661846" id2="17" version="1065353216">Push forward. Take the bridge!</text>
		<text bitflag="1345" id1="661849" id2="17" version="1065353216">Sergeant Boran is on the way!</text>
		<text bitflag="1345" id1="661850" id2="17" version="1065353216">We can take this ship!</text>
		<text bitflag="1345" id1="661851" id2="17" version="1065353216">Steady! Box them in!</text>
		<text bitflag="1345" id1="661852" id2="17" version="1065353216">Form up! Keep firing!</text>
