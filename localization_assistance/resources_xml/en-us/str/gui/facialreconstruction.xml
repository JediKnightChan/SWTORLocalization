	<file_content>
		<text bitflag="321" id1="1" id2="514535" version="1065353216">CHANGE APPEARANCE</text>
		<text bitflag="321" id1="2" id2="514535" version="1065353216">Commit all appearance changes.</text>
		<text bitflag="321" id1="3" id2="514535" version="1065353216">RESET APPEARANCE</text>
		<text bitflag="321" id1="4" id2="514535" version="1065353216">Discard all appearance changes.</text>
		<text bitflag="321" id1="5" id2="514535" version="1065353216">CHARACTER CUSTOMIZATION</text>
		<text bitflag="321" id1="6" id2="514535" version="1065353216">Adjust Appearance:</text>
		<text bitflag="321" id1="256" id2="514535" version="1065353216">Customization Cost</text>
		<text bitflag="321" id1="257" id2="514535" version="1065353216">Cartel Coin Balance</text>
		<text bitflag="321" id1="260" id2="514535" version="1065353216">When changing your species, additional customization options are free.</text>
		<text bitflag="321" id1="261" id2="514535" version="1065353216">&lt;&lt;1&gt;&gt;% off &lt;&lt;2&gt;&gt;</text>
		<text bitflag="321" id1="262" id2="514535" version="1065353216">You do not have enough Cartel Coins to make this purchase.</text>
		<text bitflag="321" id1="263" id2="514535" version="1065353216">&lt;&lt;3&gt;&gt;: &lt;&lt;1&gt;&gt;% off &lt;&lt;2&gt;&gt;</text>
		<text bitflag="321" id1="264" id2="514535" version="1065353216">Species</text>
		<text bitflag="321" id1="265" id2="514535" version="1065353216">DISCOUNTS</text>
		<text bitflag="321" id1="268" id2="514535" version="1065353216">EXIT</text>
		<text bitflag="321" id1="269" id2="514535" version="1065353216">Exit without changing character appearance.</text>
		<text bitflag="321" id1="270" id2="514535" version="1065353216">Unlock Cost</text>
		<text bitflag="321" id1="271" id2="514535" version="1065353216">Credits Balance</text>
		<text bitflag="321" id1="272" id2="514535" version="1065353216">Are you sure you wish to make these changes?  Your appearance will be updated.</text>
		<text bitflag="321" id1="273" id2="514535" version="1065353216">Species</text>
		<text bitflag="321" id1="274" id2="514535" version="1065353216">Select a species. You may purchase the Legacy species unlock as well.</text>
		<text bitflag="321" id1="275" id2="514535" version="1065353216">Body Type</text>
		<text bitflag="321" id1="276" id2="514535" version="1065353216">Select a body type.</text>
		<text bitflag="321" id1="277" id2="514535" version="1065353216">Head</text>
		<text bitflag="321" id1="278" id2="514535" version="1065353216">Select a head shape.</text>
		<text bitflag="321" id1="279" id2="514535" version="1065353216">Scars</text>
		<text bitflag="321" id1="280" id2="514535" version="1065353216">Change facial scarring.</text>
		<text bitflag="321" id1="281" id2="514535" version="1065353216">Complexion</text>
		<text bitflag="321" id1="282" id2="514535" version="1065353216">Change skin complexion.</text>
		<text bitflag="321" id1="283" id2="514535" version="1065353216">Eye Color</text>
		<text bitflag="321" id1="284" id2="514535" version="1065353216">Select an eye color.</text>
		<text bitflag="321" id1="285" id2="514535" version="1065353216">Select a hair style.</text>
		<text bitflag="321" id1="286" id2="514535" version="1065353216">Cybernetics</text>
		<text bitflag="321" id1="287" id2="514535" version="1065353216">Select a cybernetic style.</text>
