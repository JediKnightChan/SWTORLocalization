		<text bitflag="321" id1="1" id2="708245" version="1065353216">Featured</text>
		<text bitflag="321" id1="2" id2="708245" version="1065353216">Appearance Customizations</text>
		<text bitflag="321" id1="3" id2="708245" version="1065353216">Pets &amp; Companions</text>
